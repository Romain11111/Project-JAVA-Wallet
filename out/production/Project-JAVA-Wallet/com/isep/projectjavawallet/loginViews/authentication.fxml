<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="291.0" prefWidth="413.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.isep.projectjavawallet.controllers.loggin.AuthenticationController">

    <Button layoutX="16.0" layoutY="244.0" mnemonicParsing="false" onAction="#signInButtonClick" prefHeight="25.0" prefWidth="57.0" text="Sign in">
        <font>
            <Font name="System Bold" size="12.0" />
        </font>
    </Button>
    <Button layoutX="324.0" layoutY="244.0" mnemonicParsing="false" onAction="#signUpButtonClick" prefHeight="25.0" prefWidth="65.0" text="Sign up">
        <font>
            <Font name="System Bold" size="12.0" />
        </font>
    </Button>
    <Label alignment="CENTER" layoutX="7.0" layoutY="64.0" prefHeight="33.0" prefWidth="75.0" text="Username">
        <font>
            <Font name="System Bold Italic" size="14.0" />
        </font>
    </Label>
    <Label alignment="CENTER" layoutX="7.0" layoutY="125.0" prefHeight="33.0" prefWidth="75.0" text="Password">
        <font>
            <Font name="System Bold Italic" size="14.0" />
        </font>
    </Label>
    <TextField fx:id="login" layoutX="107.0" layoutY="63.0" onKeyPressed="#EnterPressed" prefHeight="35.0" prefWidth="282.0" promptText="username between 3-15">
        <font>
            <Font size="16.0" />
        </font>
    </TextField>
   <PasswordField fx:id="password" layoutX="107.0" layoutY="125.0" onKeyPressed="#EnterPressed" prefHeight="33.0" prefWidth="282.0" promptText="password between 6-15">
      <font>
         <Font size="16.0" />
      </font>
   </PasswordField>


</AnchorPane>
